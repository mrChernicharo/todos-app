{
	"env": {
		"es2020": true,
		"es6": true
	},
	"extends": ["airbnb-base"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"import/prefer-default-export": "off",
		"no-unused-vars": "warn",

		"editor.codeActionsOnSave": {
			"source.fixAll": true
		}
	}
}
